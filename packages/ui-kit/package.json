{
  "name": "@cbeyond/ui-kit",
  "description": "CBeyondVR shared UI components",
  "version": "1.1.0",
  "author": "Armelle Laine <armelle@cbeyondvr.com>",
  "main": "src/index",
  "types": "src/index",
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn run clean &&  yarn run compile && yarn run copy-assets && yarn run copy-external-typings",
    "copy-assets": "cp -rv src/assets dist/",
    "copy-external-typings": "cp -rv src/types/*.d.ts dist/types",
    "compile": "tsc -b tsconfig.build.json",
    "clean": "rimraf public-storybook dist && rm -rf tsconfig.build.tsbuildinfo",
    "start": "start-storybook -p 9009 -s ./public-storybook,./src/static",
    "storybook": "build-storybook -c .storybook -o public-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.2",
    "@mdx-js/react": "^1.5.1",
    "@mindspace-io/react": "^1.1.5",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "classnames": "^2.2.6",
    "csx": "^10.0.0",
    "fs-extra": "^8.1.0",
    "gatsby": "^2.17.11",
    "gatsby-image": "^2.2.33",
    "lodash": "^4.17.15",
    "paths.macro": "^2.0.2",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-image-gallery": "^0.9.1",
    "react-jss": "^10.0.0",
    "react-responsive-carousel": "^3.2.7",
    "react-svg-pan-zoom": "^3.8.0",
    "react-uid": "^2.2.0",
    "rxjs": "^6.5.5",
    "slash": "^3.0.0",
    "storybook": "^5.1.11",
    "svg-parser": "^2.0.2",
    "svgo": "^1.3.2",
    "typescript": "^3.6.3",
    "typestyle": "^2.0.4",
    "uri-js": "^4.2.2"
  },
  "devDependencies": {
    "@storybook/addon-info": "^5.2.6",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-blvd": "^1.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.11.1",
    "gray-matter": "^4.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  },
  "license": "MIT",
