{
  "name": "@cbeyond/villa-clementine",
  "description": "Villa Clementine Photos",
  "version": "1.0.1",
  "author": "Gerard Laine <gerard@cbeyondvr.com>",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "src"
  ],
  "private": true,
  "scripts": {
    "build": "yarn run gatsby build",
    "clean": "rimraf public .cache dist && rm -rf tsconfig.build.tsbuildinfo",
    "copy-assets": "cp -rv src/assets dist/",
    "copy-external-typings": "cp -rv src/types/*.d.ts dist/types",
    "compile": "tsc -b tsconfig.build.json",
    "serve": "yarn run gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "start": "yarn run gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md,mdx}\" \"content/**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"content/**/*.{js,jsx,ts,tsx}\" --fix",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "debug": "node --nolazy --inspect-brk ../../node_modules/.bin/gatsby develop",
    "debugw": "node --nolazy --inspect-brk ./../../node_modules/gatsby-cli/lib/index.js develop",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "starts": "start-storybook -p 9009 -s ./public-storybook,./src/static",
    "storybook": "build-storybook -c .storybook -o public-storybook",
    "cleans": "rimraf public-storybook dist",
    "gql-gen": "gql-gen"
  },
  "resolutions": {
    "sharp": "^0.23.1",
    "favicons/**/sharp": "^0.23.1",
    "favicons/**/react": "^16.2.0"
  },
